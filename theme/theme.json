{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#015FB6",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#0A0A0A",
					"name": "Neutral 100",
					"slug": "neutral-100"
				},
				{
					"color": "#2C2C2A",
					"name": "Neutral 90",
					"slug": "neutral-90"
				},
				{
					"color": "#8D8E8E",
					"name": "Neutral 30",
					"slug": "neutral-30"
				},
				{
					"color": "#EFF1F2",
					"name": "Neutral 10",
					"slug": "neutral-10"
				}
			]
		},
		"layout": {
			"contentSize": "1200px"
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "\"Playfair Display\"",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/playfair-display/playfair-display-400-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Playfair Display\", serif",
					"name": "Playfair Display",
					"slug": "playfair-display"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-400-normal.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-500-normal.woff2"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/montserrat/montserrat-600-normal.woff2"
							]
						}
					],
					"fontFamily": "Montserrat, sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				}
			],
			"fontSizes": [
				{
					"name": "Caption",
					"size": "12px",
					"slug": "caption"
				},
				{
					"name": "Body Two",
					"size": "14px",
					"slug": "body-two"
				},
				{
					"name": "Body One",
					"size": "16px",
					"slug": "body-one"
				},
				{
					"name": "Subtitle Two",
					"size": "18px",
					"slug": "subtitle-two"
				},
				{
					"name": "Subtitle One",
					"size": "20px",
					"slug": "subtitle-one"
				},
				{
					"name": "Title One",
					"size": "40px",
					"slug": "title-one"
				},
				{
					"name": "Display",
					"size": "56px",
					"slug": "display"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--playfair-display)",
					"fontSize": "var(--wp--preset--font-size--title-one)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--body-one)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"letterSpacing": "1px"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--neutral-10)",
			"text": "var(--wp--preset--color--neutral-90)"
		},
		"elements": {
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--neutral-100)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontSize": "var(--wp--preset--font-size--display)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"letterSpacing": "1px"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--neutral-100)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--playfair-display)",
					"fontStyle": "normal",
					"fontWeight": "700"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--neutral-90)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--montserrat)",
			"fontStyle": "normal",
			"fontWeight": "400",
			"letterSpacing": "1px"
		}
	}
}
